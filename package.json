{
  "name": "catnip",
  "version": "1.0.0",
  "description": "Java ByteCode interpreter (Partial)",
  "main": "index.js",
  "author": "Igor Labobrie <ilaborie@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npx parcel src/index.html",
    "build": "npx parcel build src/index.html --public-url .",
    "lint": "npx tslint --project . --fix --format stylish",
    "format": "prettier --parser typescript --write",
    "precommit": "npx lint-staged",
    "test": "npx jest",
    "publish": "rm -rf dist/ && npm run build && npx gh-pages -d dist"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.0",
    "prettier": "^1.11.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/test/*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "gh-pages": "^1.1.0"
  }
}
